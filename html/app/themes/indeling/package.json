{
  "name": "template",
  "version": "1.0.0",
  "description": "This is my webpack template for Wordpress",
  "main": "index.js",
  "scripts": {
    "start": "node webpack-config/modes/mode.dev",
    "build": "node webpack-config/modes/mode.build",
    "build-support": "cross-env AUTOPREFIXER_GRID=autoplace node node webpack-config/modes/mode.build node webpack-config/modes/mode.extended"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tellgat/webpack-template-wordpress.git"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "merge",
    "bable",
    "bable7",
    "scss"
  ],
  "author": "Tellgat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tellgat/webpack-template-wordpress/issues"
  },
  "homepage": "https://github.com/Tellgat/webpack-template-wordpress#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.10.3",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "file-loader": "^5.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "minimatch": "^3.0.4",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "querystring": "^0.2.0",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.34.1",
